#include "stdio.h"

//å®ç°strlen
int mystrlen(char* str)
{
    char* start = str;
    char* end = str;
    while (*end != '\0') 
    {   
        end++;   
    }
    return end - start;

}



int main()
{
    char arr[] = "bit";
    int len = mystrlen(arr);
    printf("%d\n", len);
    
    return 0;
}
