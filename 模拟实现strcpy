 char* mystrcpy(char* dest,const char* src)
 {
 	//加一句判断是否为空指针
        或者assert.h
 	char* ret = dest;
	while((*dest++ = *src++))
	{
		;		
	}
	return ret;
 }


 int main()
 {
	 char arr1[] = "##############";
	 char arr2[] = "bit";
	 
	 printf("%s\n", mystrcpy(arr1, arr2));
 }
