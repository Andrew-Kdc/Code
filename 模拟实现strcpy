 void mystrcpy(char* dest,char* src)
 {
	while((*dest++ = *src++))
	{
		;		
	}
 }


 int main()
 {
	 char arr1[] = "##############";
	 char arr2[] = "bit";
	 mystrcpy(arr1, arr2);
	 printf("%s\n", arr1);
 }
