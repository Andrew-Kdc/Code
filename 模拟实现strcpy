#include "stdio.h"
//#include "string.h"

//模拟实现strcpy
 
 void mystrcpy(char* dest,char* src)
 {
	while(*src != '\0')
	{
		*dest = *src;
		src++;
		dest++;
	}
	*dest = *src;
 }


 int main()
 {
	 char arr1[] = "##############";
	 char arr2[] = "bit";
	 mystrcpy(arr1, arr2);
	 printf("%s\n", arr1);
 }
