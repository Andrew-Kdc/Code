
    #include<stdio.h>
    #include<math.h>
    int main()
    {   // 回文数
    	int x,s,y=0; // s是商，y是余数
    	/*********begin*********/
    	scanf("%d",&x);
    	s = x;
    	while(s>0){
    		y = y*10 + s%10; 
    		s = s/10;
    	}
    	if(y == x){
    		printf("Yes");
    	}else{
    		printf("No");
    	}
    	/*********end*********/
    	return 0;
    }

